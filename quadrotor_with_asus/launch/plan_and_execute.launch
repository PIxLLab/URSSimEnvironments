<launch>
  <!-- rosrun action_controller action_controller -->
  <!-- <node name="action_controller" pkg="action_controller" type="action_controller" /> -->

  <!-- Run the main MoveIt executable -->
  <include file="$(find quadrotor_with_asus)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="true"/>
    <arg name="fake_execution" value="false"/>
    <arg name="debug" value="true"/>
  </include>

  <!-- Visualization RVIZ -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find quadrotor_with_asus)/launch/moveit.rviz" output="screen"/>
</launch>