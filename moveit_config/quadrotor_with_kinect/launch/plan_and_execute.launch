<launch>
  <!-- Run the main MoveIt executable -->
  <include file="$(find quadrotor_with_kinect)/launch/move_group.launch">
    <arg name="allow_trajectory_execution" value="true"/>
    <arg name="fake_execution" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <!-- Visualization RVIZ -->
  <node pkg="rviz" type="rviz" name="rviz" args="-d $(find quadrotor_with_kinect)/launch/moveit.rviz" output="screen"/>
</launch>